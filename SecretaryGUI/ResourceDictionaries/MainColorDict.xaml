<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HCI_SIMS_PROJEKAT.ResourceDictionaries">

    <Style TargetType="Grid" x:Key="WhiteMainColor">
        <Setter Property="Background" Value="White"></Setter>
    </Style>

    <Style TargetType="Grid" x:Key="DarkMainColor">
        <Setter Property="Background" Value="#FFD4D0D0"></Setter>
    </Style>

</ResourceDictionary>